{
  "name": "vite-vanilla-ts-template",
  "description": "A minimalist template for building a library with Vite and TypeScript",
  "author": "Brian Ng",
  "keywords": [
    "vite",
    "typescript",
    "library"
  ],
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/node": "^20.16.11",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9.12.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.11.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  }
}
